<!DOCTYPE html>
<html>
<head>
  <title>FinishMySideProject2</title>
  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>

<body>

<header>

  <div class="image1">
    <%= image_tag("redArrow.png") %>
  </div>

  <div class="image3">
    <%= image_tag("greenArrow.png") %>
  </div>

  <div id="topLinks">

    <%= link_to "About", about_path %>
    <%= link_to "Terms of Use", terms_path %>
    <%= link_to "privacy policy", privacy_path %>
    <%= link_to "- Login -", login_path %>
  </div>

  <div class="siteTitle">
    <%= link_to "FinishMySideProject.com", index_path %>
  </div>

  <div class="bottomLinks">
    <%= link_to "Browse", side_projects_path %>
    <%= link_to "submit", new_side_project_path %>
  </div>

  <div id="headerBackground">
  </div>

</header>


<div id="siteContainer">

  <%= yield %>
</div>


<footer>
  <%= link_to "Terms of Use", terms_path %>
  <%= link_to "Our Privacy Policy", privacy_path %>
  <%= link_to "Admin", admin_path() %>
</footer>

</body>

</html>
